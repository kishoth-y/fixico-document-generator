{
	"name": "kishoth-y/fixico-document-generator",
	"description": "Generate documentation for your Laravel application using OpenAI",
	"type": "library",
	"version": "1.0.0",
	"require": {
		"php": "^7.3|^8.0",
		"illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
		"illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
	},
	"autoload": {
		"psr-4": {
			"Docudoodle\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Docudoodle\\DocudoodleServiceProvider"
			]
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"php-mock/php-mock-phpunit": "^2.13"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Peter Day",
			"email": "peterday.main@gmail.com"
		}
	],
	"homepage": "https://github.com/genericmilk/docudoodle",
	"keywords": [
		"documentation",
		"php",
		"openai",
		"generator"
	]
}
